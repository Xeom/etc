# Pretty exponents
<asciicircum> <j> : "ⁱ" U2071
<asciicircum> <n> : "ⁿ" U207F

# Zero width space
<Multi_key> <space> : "​" U200B
include "%L"

#Logic
<Multi_key> <l> <I>              : "⇒"
<Multi_key> <l> <i>              : "→"
<Multi_key> <l> <S>              : "⊃"
<Multi_key> <l> <underscore>     : "⇔"
<Multi_key> <l> <equal>          : "≡"
<Multi_key> <l> <exclam> <equal> : "≢"
<Multi_key> <l> <minus>          : "↔"
<Multi_key> <l> <n>              : "¬"
<Multi_key> <l> <x>              : "⊕"
<Multi_key> <l> <t>              : "⊤"
<Multi_key> <l> <a>              : "∀"
<Multi_key> <l> <e>              : "∃"
<Multi_key> <l> <d>              : "≔"
<Multi_key> <l> <p>              : "⊢"
<Multi_key> <l> <P>              : "⊨"
<Multi_key> <l> <m>              : "·" 
<Multi_key> <l> <T>              : "∴"
<Multi_key> <l> <b>              : "∵"
<Multi_key> <l> <f>              : "∝"

